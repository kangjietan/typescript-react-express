{
  "compilerOptions": {
    "outDir": "../../dist/client",
    "module": "esnext",
    "target": "es6",
    "lib": [
      "es6",
      "dom",
      "esnext.asynciterable"
    ],
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "build",
    "scripts",
    "acceptance-tests",
    "../config/webpack",
    "jest",
    "src/setupTests.ts"
  ]
}